      </tr>
    </thead>
    <tbody>
      <%- if @pendings.length == 0 -%>
    <td></td>
      <td></td>
      <td> <%= l(:label_quest_no_pending) %> </td>
      <td></td>
      <%- else -%>
      <%- odd=0 -%>
      <%- for pending in @pendings -%>
      <%- case pending.quest_id
              when 1 
                quest=l(:quest1) 
              when 2 
                quest=l(:quest2_label_0) 
              when 3 
                quest=l(:quest3) 
              when 4 
                quest=l(:quest4) 
              when 5 
                quest=l(:quest5) 
              when 6 
                quest=l(:quest6) 
              when 7 
                quest=l(:quest7_label_0) 
              when 8 
                quest=l(:quest8_label_0)  
              when 9 
                quest=l(:quest9_label_0)  
              when 10 
                quest=l(:quest10_label_0)  
              when 11 
                quest=l(:quest11_label_0)
              when 12 
                quest=l(:quest12_label_0)
              end -%>
      <%- if odd == 1 -%>
    <tr class="odd">
      <%- odd=0-%>
      <%-else -%>
    <tr>
      <%- odd=1 end -%>
      <%- controlador="quest"+ pending.quest_id.to_s -%>
      <td><%= l_date(TzTime.zone.utc_to_local(pending.created_on))%></td>
      <td> <%= pending.quest_id.to_s %> </td>
      <td><%= quest %></td>
      <td><a href="/pendings/delete/<%= pending.id.to_s %>d<%= id %>"><%= image_tag 'delete.png' %></a></td>
    </tr>
    <%- end -%>
    <%- end -%>
    </tbody>
    <td></td>
    <thead>
      <tr>
        <th scope="col"><div align="left"><%=l(:label_filled) %></div></th>
