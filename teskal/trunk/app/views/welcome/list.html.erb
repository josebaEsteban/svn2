<h2> Instalaciones</h2>
<center>
<table cellspacing="0">
  <!-- <caption><%=l(:quest2)%></caption> -->
  <thead>
    <tr>
      <th scope="col"><div align="left"><%=l(:label_quest)%></div></th>
      <th scope="col"><div align="left"><%= l(:label_competition) %></div></th>
      <th scope="col"><div align="left"><%= l(:label_filled) %></div></th>
    </tr>
  </thead>
  <tbody>
    <% odd=0 %>
    <% for answer in @answers %>
    <% case  answer.quest_id
  when 1 
  quest=l(:quest1) 
  when 2 
  quest=l(:quest2_label_0) 
  when 3 
  quest=l(:quest3) 
  when 4 
  quest=l(:quest4) 
  when 4 
  quest=l(:quest5) 
  when 5 
  quest=l(:quest6) 
  when 6 
  quest=l(:quest6) 
  when 7 
  quest=l(:quest7_label_0) 
  when 8 
  quest=l(:quest8)  
	end %>
    <% if odd==1 %>
<tr class="odd">
      <% odd=0%>
      <%else %>
       <tr> 
      <% odd=1 end %>
      <td><%= quest %></td>
      <td><%= answer.competition %></td>
      <td><%= l_datetime(answer.created_on)%></td>
      <% controlador="quest"+ answer.quest_id.to_s %>
      <td><%= link_to 'Show', :controller  => controlador, :action => 'show', :id => answer %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'Back', answers_path %>