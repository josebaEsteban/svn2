<h1>Listing Tests</h1>

<table>
  <tr>
  </tr>
  
<% for answer in @answers %>
  <tr>
    <td><%= link_to 'Show', answer_path(answer) %></td>
    <td><%= link_to 'Edit', edit_answer_path(answer) %></td>
    <td><%= link_to 'Destroy', answer_path(answer), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

