			          when 11 
			            quest=l(:quest11_label_0)
			          when 12 
			            quest=l(:quest12_label_0)
			          end -%>
      <%- if odd == 1 -%>
    <tr class="odd">
      <%- odd=0-%>
      <%-else -%>
    <tr>
      <%- odd=1 end -%>
      <%- controlador="quest"+ answer.quest_id.to_s -%>
      <td><%= l_date(TzTime.zone.utc_to_local(answer.created_on))%></td>
      <td> <%= answer.quest_id.to_s %> </td>
      <td><%= link_to quest, :controller  => controlador, :action => 'show', :id => answer %></td>
      <td> <a href="/answers/switch/<%= answer.id.to_s %>">	
			      	<%- if answer.browse -%>
			      		<%= image_tag 'eye.png' %>
			      <%- else -%>
			      		<%= image_tag 'eye_crossed.png' %> 
			      <%- end -%></a></td>
    </tr>
    <%- end -%>
    <%- end -%>
