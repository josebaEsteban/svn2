<%- TzTime.zone=@athlete.timezone -%>
<div class="splitcontentright">
 
    <table cellspacing="0">
      <thead>
        <tr>
          <th scope="col"></th>
          <th scope="col"><div align="left"><%=l(:label_filled) %></div></th>
          <th scope="col"><div align="left"><%=l(:label_quest)%></div></th>
        </tr>
      </thead>
      <tbody>
        <%- odd=0 -%>
        <%- for pending in @pendings -%>
        <%- case pending.quest_id
              when 1 
                quest=l(:quest1) 
              when 2 
                quest=l(:quest2_label_0) 
              when 3 
                quest=l(:quest3) 
              when 4 
                quest=l(:quest4) 
              when 5 
                quest=l(:quest5) 
              when 6 
                quest=l(:quest6) 
              when 7 
                quest=l(:quest7_label_0) 
              when 8 
                quest=l(:quest8_label_0)  
              when 9 
                quest=l(:quest9_label_0)  
              when 10 
                quest=l(:quest10_label_0)  
              when 11 
                quest=l(:quest11_label_0)
              when 12 
                quest=l(:quest12_label_0)
              end -%>
        <%- if odd == 1 -%>
        <tr class="odd">
          <%- odd=0-%>
          <%-else -%>
        <tr>
          <%- odd=1 end -%>
          <%- controlador="quest"+ pending.quest_id.to_s -%>
          <td><a href="/pendings/delete/<%= pending.id.to_s %>d<%= id %>"<%= image_tag 'delete.png' %></a></td>
          <td><%= l_datetime(TzTime.zone.utc_to_local(pending.created_on))%></td>
          <td><%= quest %></td>
        </tr>
        <%- end -%>
      </tbody>
    </table>
 <center>
    <table cellspacing="0">
      <thead>
        <tr>
          <th scope="col"></th>
          <th scope="col"><div align="left"><%=l(:label_filled) %></div></th>
          <th scope="col"><div align="left"><%=l(:label_quest)%></div></th>
          <th scope="col"><div align="left"><%=l(:label_competition) %></div></th>
        </tr>
      </thead>
      <tbody>
        <%- odd=0 -%>
        <%- for answer in @answers -%>
        <%- case answer.quest_id
              when 1 
                quest=l(:quest1) 
              when 2 
                quest=l(:quest2_label_0) 
              when 3 
                quest=l(:quest3) 
              when 4 
                quest=l(:quest4) 
              when 5 
                quest=l(:quest5) 
              when 6 
                quest=l(:quest6) 
              when 7 
                quest=l(:quest7_label_0) 
              when 8 
                quest=l(:quest8_label_0)  
              when 9 
                quest=l(:quest9_label_0)  
              when 10 
                quest=l(:quest10_label_0)  
              when 11 
                quest=l(:quest11_label_0)
              when 12 
                quest=l(:quest12_label_0)
              end -%>
        <%- if odd == 1 -%>
        <tr class="odd">
          <%- odd=0-%>
          <%-else -%>
        <tr>
          <%- odd=1 end -%>
          <%- controlador="quest"+ answer.quest_id.to_s -%>
          <td> <a href="/answers/switch/<%= answer.id.to_s %>"
	
          	<%- if answer.browse == 1 -%>
          		<%= image_tag 'eye.png' %>
          <%- else -%>
          		<%= image_tag 'eye_crossed.png' %> 
          <%- end -%></a></td>
